{
  "name": "deer-ui",
  "version": "0.0.1",
  "license": "MIT",
  "description": "基于 ant-design 封装的偏业务组件",
  "keywords": [
    "react",
    "ant-design",
    "components",
    "deer-ui"
  ],
  "files": [
    "es",
    "lib"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "homepage": "https://github.com/532758463/deer-ui.git",
  "bugs": {
    "url": "https://github.com/532758463/deer-ui.git/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/532758463/deer-ui.git"
  },
  "scripts": {
    "precommit": "lint-staged",
    "commit": "git-cz",
    "start": "dumi dev",
    "site": "dumi build",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "build": "lessc index.less index.css -x",
    "docs:build": "dumi build",
    "docs-dev:build": "UMI_ENV=dev dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish",
    "lint:style": "stylelint packages/**/*.less",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "peerDependencies": {
    "antd": ">=4.0.0",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "@pansy/hooks": "^2.2.0",
    "@pansy/policy": "^0.5.0",
    "@pansy/react-fullscreen": "1.3.0",
    "@pansy/react-watermark": "2.1.0",
    "classnames": "^2.2.6",
    "lodash": "^4.17.20",
    "resize-observer-polyfill": "^1.5.1"
  },
  "devDependencies": {
    "@umijs/plugin-locale": "^0.10.8",
    "@types/classnames": "^2.2.11",
    "@types/lodash": "^4.14.165",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "antd": "^4.9.1",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "babel-plugin-import": "^1.13.3",
    "del": "^6.0.0",
    "dumi": "^1.0.37",
    "father-build": "^1.19.1",
    "husky": "4.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "typescript": "^4.1.2",
    "commitizen": "^4.2.2",
    "lint-staged": "^10.2.0",
    "git-cz": "^4.7.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier",
      "commit-msg": "yarn lint:commit"
    }
  },
  "browserslist": [
    "last 2 version",
    "Firefox ESR",
    "> 1%",
    "ie >= 9"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=10.9.0",
    "yarn": ">=1.3.2"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  }
}
