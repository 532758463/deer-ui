{
  "name": "deer-ui",
  "version": "0.0.1",
  "license": "MIT",
  "description": "基于 ant-design 封装的偏业务组件",
  "keywords": [
    "react",
    "ant-design",
    "components",
    "deer-ui"
  ],
  "files": [
    "es",
    "lib"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "homepage": "https://github.com/532758463/deer-ui.git",
  "bugs": {
    "url": "https://github.com/532758463/deer-ui.git/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/532758463/deer-ui.git"
  },
  "scripts": {
    "precommit": "lint-staged",
    "commit": "git-cz",
    "start": "dumi dev",
    "site": "dumi build",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "build:less": "lessc src/style/index.less lib/index.css es/index.css -x",
    "build:es": "tsc --project tsconfig.build.json --module esnext --outDir es",
    "build:lib": "tsc --project tsconfig.build.json --module commonjs --outDir lib",
    "prebuild": "rimraf es lib",
    "build-all": "cross-env NODE_ENV=production && npm run prebuild && npm run build:es && npm run build:lib && gulp",
    "docs:deploy": "gh-pages -d dist",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish",
    "lint:style": "stylelint packages/**/*.less",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "peerDependencies": {
    "antd": ">=4.0.0",
    "mobx": ">=4",
    "mobx-react-lite": ">=3.0.0",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "@pansy/hooks": "^2.2.0",
    "@pansy/policy": "^0.5.0",
    "@pansy/react-fullscreen": "1.3.0",
    "@pansy/react-watermark": "2.1.0",
    "classnames": "^2.2.6",
    "lodash": "^4.17.20",
    "resize-observer-polyfill": "^1.5.1"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.11",
    "@types/lodash": "^4.14.165",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/plugin-locale": "^0.10.8",
    "antd": "^4.12.2",
    "babel-plugin-import": "^1.13.3",
    "commitizen": "^4.2.2",
    "cross-env": "^7.0.2",
    "del": "^6.0.0",
    "dumi": "^1.0.37",
    "father-build": "^1.19.1",
    "gh-pages": "^3.1.0",
    "git-cz": "^4.7.6",
    "gulp": "^4.0.2",
    "gulp-less": "^4.0.1",
    "husky": "4.3.0",
    "less-plugin-autoprefix": "^2.0.0",
    "lint-staged": "^10.2.0",
    "mobx": "^6.1.5",
    "mobx-react-lite": "^3.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "stylelint": "^13.0.0",
    "stylelint-config-prettier": "^8.0.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.1.0",
    "stylelint-order": "^4.0.0",
    "ts-node": "^8.9.1",
    "typescript": "^4.1.2",
    "umi-plugin-gh-pages":"^0.2.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "browserslist": [
    "last 2 version",
    "Firefox ESR",
    "> 1%",
    "ie >= 9"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=10.9.0",
    "yarn": ">=1.3.2"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  }
}
